#ifndef NODE_H_
#define NODE_H_
#include <string>
using namespace std;
struct edge
{
	unsigned int headindex;
	unsigned int tailindex;

	int weight;

	edge(unsigned int headindex=0u, unsigned int tailindex=0u, int weight=0):
		headindex(headindex), tailindex(tailindex), weight(weight)
	{
	}
};

struct vertex
{

};



#endif /* NODE_H_ */
